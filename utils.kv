#: import get_color_from_hex kivy.utils.get_color_from_hex


<Overlay>:
    opacity: 0.8
    md_bg_color:0,0,0,1
    canvas:
        Color:
            rgba:get_color_from_hex("#002863")
        Ellipse:
            angle_start:0
            angle_end:360
            pos:self.center
            size:self.rad, self.rad